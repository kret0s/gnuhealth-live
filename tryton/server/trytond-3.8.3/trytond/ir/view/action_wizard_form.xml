<form string="Wizard">
    <label name="name"/>
    <field name="name"/>
    <label name="active"/>
    <field name="active" xexpand="0" width="100"/>
    <notebook colspan="4">
        <page string="General" id="general">
            <label name="wiz_name"/>
            <field name="wiz_name"/>
            <label name="model"/>
            <field name="model"/>
            <label name="icon"/>
            <field name="icon"/>
            <newline/>
